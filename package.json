{
  "name": "polli",
  "version": "1.0.0",
  "description": "Polls redux react.js application",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "NODE_ENV='production' webpack"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/syntax-dynamic-import"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moisestech/polli.git"
  },
  "keywords": [
    "reactjs",
    "reactredux",
    "reduxthunk",
    "polls",
    "questionaires"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moisestech/polli/issues"
  },
  "homepage": "https://github.com/moisestech/polli#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@reduxjs/toolkit": "^1.4.0",
    "babel-loader": "^8.1.0",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^4.3.0",
    "html-webpack-plugin": "^4.5.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "react": "^17.0.0-rc.2",
    "react-dom": "^17.0.0-rc.2",
    "react-redux": "^7.2.1",
    "react-redux-loading-bar": "^5.0.0",
    "react-router": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  }
}
